{
  if (arguments == null) {
    return "null";
  }
  StringBuilder argBuf=new StringBuilder(arguments.length);
  argBuf.append('[');
  for (int i=0; i < arguments.length; i++) {
    if (i > 0) {
      argBuf.append(", ");
    }
    argBuf.append(format(arguments[i],false));
  }
  argBuf.append(']');
  return argBuf.toString();
}
