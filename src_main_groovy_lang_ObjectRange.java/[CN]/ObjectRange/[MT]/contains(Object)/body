{
  final Iterator it=new StepIterator(this,1);
  if (value == null) {
    return false;
  }
  while (it.hasNext()) {
    try {
      if (DefaultTypeTransformation.compareEqual(value,it.next()))       return true;
    }
 catch (    ClassCastException e) {
      return false;
    }
  }
  return false;
}
