{
  if (iterator.hasNext()) {
    buffer.addChar(OPEN_BRACKET);
    Object it=iterator.next();
    writeObject(it,buffer);
    while (iterator.hasNext()) {
      it=iterator.next();
      buffer.addChar(COMMA);
      writeObject(it,buffer);
    }
    buffer.addChar(CLOSE_BRACKET);
  }
 else {
    buffer.addChars(EMPTY_LIST_CHARS);
  }
}
